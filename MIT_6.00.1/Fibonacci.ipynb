   "source": [
    "def fib(x):\n",
    "    '''\n",
    "    assumes x is an int >= 0\n",
    "    \n",
    "    returns Fibonacci of x\n",
    "    '''\n",
    "    \n",
    "    if x == 0 or x == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return fib(x - 1) + fib(x - 2)"
   ]
